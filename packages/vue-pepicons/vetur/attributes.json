  "pepicon/type": {
    "type": "string",
    "description": "Either 'pop' or 'print'",
    "options": [
      "pop",
      "print"
    ]
  },
  "pepicon/color": {
    "type": "string",
    "description": "You can pass a hex or rgba color, this is applied to the svg tag\n\n(you can also just manually apply a color via style or a class)"
  },
  "pepicon/opacity": {
    "type": "number",
    "description": "A number between 0 and 1; where 0 is transparent\n- in \"pop\" style: opacity will be set to the entire icon\n- in \"print\" style: opacity will be set to the colored drop shadow"
  },
  "pepicon/stroke": {
    "type": "string",
    "description": "The stroke color is only applied on 'print' type icons and is black by default"
  },
  "pepicon/size": {
    "type": " 'sm' | 'md' | 'lg' | 'xl' | number | string ",
    "description": "When you pass a size, it's applied via the style attribute.\n- 'sm' / 'md' / 'lg' / 'xl' which becomes 20 / 24 / 30 / 36 px\n- a number for a \"px\" size. eg. 10 will become 10px\n- a string for any size value that will be applied to the width\n\n(you can also just manually apply a width & height via style or a class)",
    "options": [
      "sm",
      "md",
      "lg",
      "xl"
    ]
  }
