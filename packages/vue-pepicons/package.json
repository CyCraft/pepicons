{
  "name": "vue-pepicons",
  "version": "1.0.4",
  "sideEffects": false,
  "author": "Luca Ban - Mesqueeb",
  "funding": "https://github.com/sponsors/mesqueeb",
  "license": "CREATIVE COMMONS ATTRIBUTION 4.0 INTERNATIONAL LICENSE",
  "description": "Vue component for Pepicons",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "gen:types:vetur": "vue-int -i src/Pepicon.vue -o dist/vetur",
    "gen:types:ts": "vue-tsc --declaration --emitDeclarationOnly",
    "build": "rimraf dist && vite build && npm run gen:types:ts && npm run gen:types:vetur"
  },
  "peerDependencies": {
    "vue": "^3.2.26"
  },
  "dependencies": {
    "pepicons": "^1.0.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.7.3",
    "vue": "^3.2.26",
    "vue-intellisense": "^0.3.0",
    "vue-tsc": "^0.29.8"
  },
  "homepage": "https://github.com/cycraft/pepicons",
  "repository": {
    "type": "git",
    "url": "https://github.com/cycraft/pepicons"
  },
  "bugs": "https://github.com/cycraft/pepicons/issues",
  "vetur": {
    "tags": "dist/vetur/tags.json",
    "attributes": "dist/vetur/attributes.json"
  }
}
