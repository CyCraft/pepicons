{
  "name": "@pepicons/vue",
  "type": "module",
  "version": "2.0.4",
  "sideEffects": false,
  "author": "Luca Ban - Mesqueeb",
  "funding": "https://github.com/sponsors/mesqueeb",
  "license": "CREATIVE COMMONS ATTRIBUTION 4.0 INTERNATIONAL LICENSE",
  "description": "Vue component for Pepicons",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "build": "del-cli dist && npm run build:vite && npm run gen:types:ts && npm run gen:types:vetur",
    "dev": "vite --force --config vite.config.dev.ts",
    "build:vite": "vite build",
    "gen:types:vetur": "vue-int -i src/components/Pepicon.vue -o dist/vetur",
    "gen:types:ts": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^3.2.39"
  },
  "dependencies": {
    "pepicons": "^2.0.4"
  },
  "homepage": "https://github.com/cycraft/pepicons",
  "repository": {
    "type": "git",
    "url": "https://github.com/cycraft/pepicons"
  },
  "bugs": "https://github.com/cycraft/pepicons/issues",
  "vetur": {
    "tags": "dist/vetur/tags.json",
    "attributes": "dist/vetur/attributes.json"
  },
  "gitHead": "b9d99854c5f1a88b1bd6b50ed81b249feea5fcd9"
}
