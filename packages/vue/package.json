{
  "name": "@pepicons/vue",
  "type": "module",
  "version": "1.1.1",
  "sideEffects": false,
  "author": "Luca Ban - Mesqueeb",
  "funding": "https://github.com/sponsors/mesqueeb",
  "license": "CREATIVE COMMONS ATTRIBUTION 4.0 INTERNATIONAL LICENSE",
  "description": "Vue component for Pepicons",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/vue/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/vue/src/index.d.ts"
    }
  },
  "scripts": {
    "build": "rimraf dist && npm run build:vite && npm run gen:types:ts && npm run gen:types:vetur",
    "dev": "vite --force",
    "build:vite": "vite build",
    "gen:types:vetur": "vue-int -i src/components/Pepicon.vue -o dist/vetur",
    "gen:types:ts": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^3.2.37"
  },
  "dependencies": {
    "pepicons": "^1.1.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "@viteplay/plugin": "^0.0.26",
    "@viteplay/vue": "^0.0.26",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vue": "^3.2.37",
    "vue-intellisense": "^0.3.1",
    "vue-tsc": "^0.38.2"
  },
  "homepage": "https://github.com/cycraft/pepicons",
  "repository": {
    "type": "git",
    "url": "https://github.com/cycraft/pepicons"
  },
  "bugs": "https://github.com/cycraft/pepicons/issues",
  "vetur": {
    "tags": "dist/vetur/tags.json",
    "attributes": "dist/vetur/attributes.json"
  },
  "gitHead": "66e66f190186b07b479f78282645871be1bd4d14"
}
