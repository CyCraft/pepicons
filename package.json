{
  "name": "pepicons",
  "private": true,
  "author": "Luca Ban - Mesqueeb",
  "scripts": {
    "dev": "cd packages/pepicons.com && npm run dev",
    "build": "npm run prepare-pepicons && lerna run build",
    "publish": "npm run build && lerna publish",
    "prepare-pepicons": "node ./build/preparePepicons.js",
    "dependencies-install": "npm i && lerna bootstrap",
    "dependencies-update-all": "ncu --semverLevel major -u && lerna exec 'ncu --semverLevel major -u' && npm run dependencies-install",
    "dependencies-check-for-updates": "ncu --semverLevel major && lerna exec 'ncu --semverLevel major'",
    "dependencies-reinstall-all": "rimraf node_modules && lerna clean -y && npm run dependencies-install"
  },
  "bugs": {
    "url": "https://github.com/mesqueeb/planetar/issues"
  },
  "homepage": "https://github.com/mesqueeb/planetar#readme",
  "devDependencies": {
    "@types/node": "^10.17.27",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-eslint": "^10.1.0",
    "case-anything": "^1.1.1",
    "copyfiles": "^2.3.0",
    "debounce": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-vue": "^6.2.2",
    "fast-sort": "^2.2.0",
    "lerna": "^3.22.1",
    "npm-check-updates": "^7.0.2",
    "prettier": "^2.0.5",
    "renamer": "^2.0.1",
    "replace-in-file": "^6.1.0",
    "replace-in-files-cli": "^0.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.22.1",
    "rollup-plugin-typescript2": "^0.27.1",
    "typescript": "^3.9.7"
  }
}
